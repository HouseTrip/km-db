#!/usr/bin/env ruby
=begin
    
  Send KM events to Mixpanel after the dumps have been migrated to a database.

=end

require 'rubygems'
require 'kmdb'

KMDB::Event.find_each() do |event|
  puts event
end

KMDB::User.find_each() do |user|
  puts user
end

KMDB::Property.find_each(:conditions => {:event_id => nil}) do |property|
  puts property
end